<p id="notice"><%= notice %></p>

<h1>St Estados</h1>

<table>
  <thead>
    <tr>
      <th>Orden</th>
      <th>St estado</th>
      <th>Destinos</th>
      <th>Destinos admin</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @st_estados.each do |st_estado| %>
      <tr>
        <td><%= st_estado.orden %></td>
        <td><%= st_estado.st_estado %></td>
        <td><%= st_estado.destinos %></td>
        <td><%= st_estado.destinos_admin %></td>
        <td><%= link_to 'Show', st_estado %></td>
        <td><%= link_to 'Edit', edit_st_estado_path(st_estado) %></td>
        <td><%= link_to 'Destroy', st_estado, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New St Estado', new_st_estado_path %>
