<%- if controller_name != 'sessions' && false %>
  <small><%= link_to "Ingresar", new_session_path(resource_name) %></small><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' && false %>
  <small><%= link_to "Registrar", new_registration_path(resource_name) %></small><br />
<% end %>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <small><%= link_to "Olvidó su contraseña?", new_password_path(resource_name) %></small><br />
<% end %>

<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <small><%= link_to "No recibió instrucciones de confirmación?", new_confirmation_path(resource_name) %></small><br />
<% end %>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <small><%= link_to "No recibió instrucciones de desbloqueo?", new_unlock_path(resource_name) %></small><br />
<% end %>

<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <small><%= link_to "Ingresar con #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider) %></small><br />
  <% end %>
<% end %>
