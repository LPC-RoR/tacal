<div class="list-group">
<% list.each do |item| %>
	<%= link_to text_with_badge(item[0], item[1]), {controller: controller_name, action: action_name, html_options: get_html_opts(options, 'sel', item[0])}, class: "list-group-item list-group-item-action d-flex justify-content-between align-items-center list-group-item-info #{"active" if item[0] == sel}" %>
<% end %>
</div>
