<p id="notice"><%= notice %></p>

<h1>B Claves</h1>

<table>
  <thead>
    <tr>
      <th>B clave</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @b_claves.each do |b_clave| %>
      <tr>
        <td><%= b_clave.b_clave %></td>
        <td><%= link_to 'Show', b_clave %></td>
        <td><%= link_to 'Edit', edit_b_clave_path(b_clave) %></td>
        <td><%= link_to 'Destroy', b_clave, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

