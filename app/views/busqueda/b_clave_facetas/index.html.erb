<p id="notice"><%= notice %></p>

<h1>B Clave Facetas</h1>

<table>
  <thead>
    <tr>
      <th>Modelo</th>
      <th>Campo</th>
      <th>Valor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @b_clave_facetas.each do |b_clave_faceta| %>
      <tr>
        <td><%= b_clave_faceta.modelo %></td>
        <td><%= b_clave_faceta.campo %></td>
        <td><%= b_clave_faceta.valor %></td>
        <td><%= link_to 'Show', b_clave_faceta %></td>
        <td><%= link_to 'Edit', edit_b_clave_faceta_path(b_clave_faceta) %></td>
        <td><%= link_to 'Destroy', b_clave_faceta, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

