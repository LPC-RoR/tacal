<p id="notice"><%= notice %></p>

<h1>B Indices</h1>

<table>
  <thead>
    <tr>
      <th>B clave</th>
      <th>Ref class</th>
      <th>Ref</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @b_indices.each do |b_indice| %>
      <tr>
        <td><%= b_indice.b_clave_id %></td>
        <td><%= b_indice.ref_class %></td>
        <td><%= b_indice.ref_id %></td>
        <td><%= link_to 'Show', b_indice %></td>
        <td><%= link_to 'Edit', edit_b_indice_path(b_indice) %></td>
        <td><%= link_to 'Destroy', b_indice, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New B Indice', new_b_indice_path %>
