<p id="notice"><%= notice %></p>

<h1>App Dir Dires</h1>

<table>
  <thead>
    <tr>
      <th>Parent</th>
      <th>Child</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @app_dir_dires.each do |app_dir_dir| %>
      <tr>
        <td><%= app_dir_dir.parent_id %></td>
        <td><%= app_dir_dir.child_id %></td>
        <td><%= link_to 'Show', app_dir_dir %></td>
        <td><%= link_to 'Edit', edit_app_dir_dir_path(app_dir_dir) %></td>
        <td><%= link_to 'Destroy', app_dir_dir, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New App Dir Dir', new_app_dir_dir_path %>
