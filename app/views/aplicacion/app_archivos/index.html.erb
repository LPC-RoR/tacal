<p id="notice"><%= notice %></p>

<h1>App Archivos</h1>

<table>
  <thead>
    <tr>
      <th>Archivo</th>
      <th>Owner class</th>
      <th>Owner</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @app_archivos.each do |app_archivo| %>
      <tr>
        <td><%= app_archivo.archivo %></td>
        <td><%= app_archivo.owner_class %></td>
        <td><%= app_archivo.owner_id %></td>
        <td><%= link_to 'Show', app_archivo %></td>
        <td><%= link_to 'Edit', edit_app_archivo_path(app_archivo) %></td>
        <td><%= link_to 'Destroy', app_archivo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New App Archivo', new_app_archivo_path %>
